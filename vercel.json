{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/client",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "VERCEL": "1",
    "CLIENT_URL": "https://basesnapshot.vercel.app"
  }
}
